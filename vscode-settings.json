{
    "files.autoSave": "onFocusChange",
    "files.trimFinalNewlines": true,
    "files.trimTrailingWhitespace": true,
    "files.insertFinalNewline": true,
    "editor.renderWhitespace": "boundary",
    "editor.multiCursorModifier": "ctrlCmd",
    "editor.cursorStyle": "underline",
    "editor.fontSize": 13,
    "editor.tabSize": 2,
    "editor.renderLineHighlight": "none",
    "editor.scrollbar.horizontal": "visible",
    "editor.scrollbar.vertical": "hidden",
    "editor.minimap.enabled": false,
    "editor.glyphMargin": false,
    "editor.renderIndentGuides": true,
    "editor.selectionHighlight": false,
    "window.titleBarStyle": "native",
    "workbench.editor.tabCloseButton": "left",
    "workbench.editor.limit.enabled": true,
    "workbench.editor.limit.perEditorGroup": true,
    "workbench.editor.limit.value": 8,
    "workbench.startupEditor": "none",
    "git.confirmSync": false,
    "git.autofetch": true,
    "breadcrumbs.enabled": true,
    "window.zoomLevel": 0,
    "team.showWelcomeMessage": false,
    "editor.suggestSelection": "first",
    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
    "markdownlint.customRules": [
        "{docsmsft.docs-markdown}/markdownlint-custom-rules/rules.js"
    ],
    "markdownlint.config": {
        "MD025": {
            "front_matter_title": ""
        }
    },
    "vs-kubernetes": {
        "vs-kubernetes.kubectl-path": "/usr/local/bin/kubectl",
        "vs-kubernetes.kubectl-path.windows": "c:\\ProgramData\\chocolatey\\bin\\kubectl.exe",
        "vs-kubernetes.minikube-path.mac": "/Users/bruce/.vs-kubernetes/tools/minikube/darwin-amd64/minikube",
        "vs-kubernetes.minikube-path.linux": "/home/bruce/.vs-kubernetes/tools/minikube/linux-amd64/minikube",
        "vs-kubernetes.draft-path.linux": "/home/bruce/.vs-kubernetes/tools/draft/linux-amd64/draft",
        "vs-kubernetes.kubectl-path.linux": "/home/bruce/.vs-kubernetes/tools/kubectl/kubectl",
        "vs-kubernetes.helm-path.linux": "/home/bruce/.vs-kubernetes/tools/helm/linux-amd64/helm",
        "vs-kubernetes.minikube-path.windows": "C:\\Users\\bruce\\.vs-kubernetes\\tools\\minikube\\windows-amd64\\minikube.exe"
    },
    "terraform.indexing": {
        "enabled": false,
        "liveIndexing": false,
        "delay": 500,
        "exclude": [
            ".terraform/**/*",
            "**/.terraform/**/*"
        ],
        "enable": "true"
    },
    "terraform.languageServer": {
        "external": true,
        "args": [
            "serve"
        ]
    },
    "python.jediEnabled": false,
    "python.insidersChannel": "weekly",
    "terraform.format": {

    },
    "python.languageServer": "Microsoft",
    "yaml.schemas": {
        "file:///toc.schema.json": "/toc\\.yml/i"
    },
    "terminal.integrated.fontFamily": "MesloLGS NF",
    "remote.SSH.remotePlatform": {
        "workstation": "linux"
    },
    "workbench.editor.wrapTabs": true,
    "window.title": "${activeEditorMedium}${separator}${rootPath}",
    "bracket-pair-colorizer-2.activeScopeCSS": [

        "borderStyle : solid",
        "borderWidth : 1px",
        "borderColor : {color}",
        "opacity: 0.5"
    ]
}