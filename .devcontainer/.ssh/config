Host 192.*
  User bruce
  StrictHostKeyChecking no
  UserKnownHostsFile=/dev/null
  IdentityFile ~/.ssh/id_rsa

Host *.conecrazy.io
  User bruce
  StrictHostKeyChecking no
  UserKnownHostsFile=/dev/null
  IdentityFile ~/.ssh/id_rsa

Host *.amazonaws.com
  User root
  IdentityFile ~/.ssh/id_rsa

Host gitlab.com
  HostName gitlab.com
  Preferredauthentications publickey
  User brujack2
  IdentityFile ~/.ssh/id_ed25519

Host workstation
  HostName workstation
  User bruce
  IdentityFile ~/.ssh/id_rsa
