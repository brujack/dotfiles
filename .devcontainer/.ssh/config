Host 192.168.51.*
  IdentityFile ~/.ssh/id_rsa

Host *.home.conecrazy.ca
  IdentityFile ~/.ssh/home

Host home-secure.conecrazy.ca
  ProxyCommand /opt/homebrew/bin/cloudflared access ssh --hostname %h
