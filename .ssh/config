Host 192.*
  User bruce
  StrictHostKeyChecking no
  UserKnownHostsFile=/dev/null

Host *.ca-central-1.compute.amazonaws.com
  User ubuntu
  StrictHostKeyChecking no
  UserKnownHostsFile=/dev/null

Host 172.*
  User ubuntu
  StrictHostKeyChecking no
  UserKnownHostsFile=/dev/null

Host 10.*
  User ubuntu
  StrictHostKeyChecking no
  UserKnownHostsFile=/dev/null

Host *.amazonaws.com
  User root
  IdentityFile ~/.ssh/id_rsa.pub

Host *
  HostName %h
  User root
  IdentityFile ~/.ssh/id_rsa.pub
