Host 192.168.1.2
  User root
  StrictHostKeyChecking no
  UserKnownHostsFile=/dev/null

Host 192.168.1.3
  User root
  StrictHostKeyChecking no
  UserKnownHostsFile=/dev/null

Host 192.*
  User bruce
  StrictHostKeyChecking no
  UserKnownHostsFile=/dev/null
  IdentityFile ~/.ssh/id_rsa

Host *.conecrazy.io
  User bruce
  StrictHostKeyChecking no
  UserKnownHostsFile=/dev/null
  IdentityFile ~/.ssh/id_rsa

Host *.amazonaws.com
  User root
  IdentityFile ~/.ssh/id_rsa

Host gitlab.com
  HostName gitlab.com
  Preferredauthentications publickey
  User brujack2
